{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["react-app", "airbnb"],
  "plugins": ["import", "jsx-ally", "react"],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"]}],
    "import/prefer-default-export": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}
